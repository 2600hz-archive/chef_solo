# Generated by Chef for <%= node[:fqdn] %>
# Local modifications will be overwritten.
[elff]
name=Enterprise Linux Fast Forward 5 - $basearch
baseurl=<%= node[:repo][:elff][:url] %>
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/<%= node[:repo][:elff][:key] %>

[elff-debuginfo]
name=Enterprise Linux Fast Forward 5 - $basearch - Debug
baseurl=http://download.elff.bravenet.com/5/$basearch/debug
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/<%= node[:repo][:elff][:key] %>
gpgcheck=1

[elff-source]
name=Enterprise Linux Fast Forward 5 - $basearch - Source
baseurl=http://download.elff.bravenet.com/5/SRPMS
enabled=0
gpgkey=file:///etc/pki/rpm-gpg/<%= node[:repo][:elff][:key] %>
gpgcheck=1
