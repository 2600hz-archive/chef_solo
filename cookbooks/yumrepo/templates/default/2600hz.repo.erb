# Generated by Chef for <%= node[:fqdn] %>
# Local modifications will be overwritten.

<% if node['platform_version'].to_i <= 5 %>
[2600hzrepo]
name=Centos - $releasever - 2600hz  Whistle Winkstart Repository
<% if node.chef_environment == "staging" %>
baseurl=http://repo1.2600hz.com/staging/
<% elsif node.chef_environment == "qa" %>
baseurl=http://repo1.2600hz.com/qa/
<% elsif node.chef_environment == "perf" %>
baseurl=http://repo1.2600hz.com/qa/
<% elsif node.chef_environment == "production" %>
baseurl=http://repo1.2600hz.com/prod/
<% elsif node.chef_environment == "development" %>
baseurl=http://repo1.2600hz.com/dev/
<% else  %>
baseurl=http://repo1.2600hz.com/staging/
<% end %> 
enabled=1
gpgcheck=0

[2600hz_custom]
name=Centos - $releasever - 2600hz Custom Repository
baseurl=http://rpm.2600hz.com/$releasever/$basearch
enabled=1
gpgcheck=0 
<% else %>
[2600hz_custom]
name=Centos - $releasever - 2600hz Custom Repository
baseurl=http://rpm.2600hz.com/$releasever/$basearch
enabled=1
gpgcheck=0
<% end %>
[2600hz_packages]
name=Centos - $releasever - 2600hz Custom Repository
baseurl=http://packages.2600hz.com/rpm/$releasever/$basearch
enabled=1
gpgcheck=0


<% if node.chef_environment == "qa" %>
[2600hz_private]
name=Centos - $releasever - 2600hz Private Repository
baseurl=http://private.2600hz.com/$releasever/$basearch/qa
enabled=1
gpgcheck=0
<% elsif node.chef_environment == "staging" %>
[2600hz_private]
name=Centos - $releasever - 2600hz Private Repository
baseurl=http://private.2600hz.com/$releasever/$basearch/staging
enabled=1
gpgcheck=0
<% elsif node.chef_environment == "development" %>
[2600hz_private]
name=Centos - $releasever - 2600hz Private Repository
baseurl=http://private.2600hz.com/$releasever/$basearch/dev
enabled=1
gpgcheck=0
<% elsif node.chef_environment == "production" %>
[2600hz_private]
name=Centos - $releasever - 2600hz Private Repository
baseurl=http://private.2600hz.com/$releasever/$basearch/prod
enabled=1
gpgcheck=0
<% elsif node.chef_environment == "perf" %>
[2600hz_private]
name=Centos - $releasever - 2600hz Private Repository
baseurl=http://private.2600hz.com/$releasever/$basearch/qa
enabled=1
gpgcheck=0
<% end %>
